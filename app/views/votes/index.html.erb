<h1>Listing votes</h1>

<table>
  <thead>
    <tr>
        <th>Twitter</th>
        <th>Team</th>
        <th>Sata</th>
        <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <%= content_tag_for(:tr, @votes) do |vote| %>
        <td><%= vote.user_id %></td>
        <td><%= vote.team_id %></td>
        <td><%= vote.sata %></td>
        <td><%= link_to 'Show', vote %></td>
      <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
      <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Vote', new_vote_path %>
